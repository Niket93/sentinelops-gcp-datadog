{
    "title": "SentinelOps • Executive Command Center",
    "description": "Vision → Reason → Action: Real-time AI Pipeline Observability for Executives and Engineers",
    "layout_type": "ordered",
    "reflow_type": "fixed",
    "tags": [
        "team:hackathon"
    ],
    "template_variables": [
        {
            "name": "service",
            "default": "sentinelops",
            "prefix": "service"
        }
    ],
    "widgets": [
        {
            "id": 1,
            "definition": {
                "type": "note",
                "content": "# SYSTEM HEALTH",
                "background_color": "black",
                "font_size": "24",
                "text_align": "center",
                "vertical_align": "center",
                "show_tick": false,
                "has_padding": true
            },
            "layout": {
                "x": 0,
                "y": 0,
                "width": 12,
                "height": 1
            }
        },
        {
            "id": 2,
            "definition": {
                "type": "query_value",
                "title": "HEARTBEAT",
                "title_size": "13",
                "title_align": "center",
                "requests": [
                    {
                        "q": "max:sentinel.app.heartbeat{service:$service}",
                        "aggregator": "last",
                        "conditional_formats": [
                            {
                                "comparator": ">",
                                "value": 0.5,
                                "palette": "white_on_green"
                            },
                            {
                                "comparator": "<=",
                                "value": 0.5,
                                "palette": "white_on_red"
                            }
                        ]
                    }
                ],
                "autoscale": false,
                "text_align": "center",
                "precision": 0
            },
            "layout": {
                "x": 0,
                "y": 1,
                "width": 2,
                "height": 2
            }
        },
        {
            "id": 3,
            "definition": {
                "type": "query_value",
                "title": "CLIPS PROCESSED",
                "title_size": "13",
                "title_align": "center",
                "requests": [
                    {
                        "q": "sum:sentinel.throughput.clips{service:$service}.as_count()"
                    }
                ],
                "autoscale": true,
                "text_align": "center",
                "precision": 0
            },
            "layout": {
                "x": 2,
                "y": 1,
                "width": 2,
                "height": 2
            }
        },
        {
            "id": 4,
            "definition": {
                "type": "query_value",
                "title": "LLM CALLS",
                "title_size": "13",
                "title_align": "center",
                "requests": [
                    {
                        "q": "sum:sentinel.llm.calls{service:$service}.as_count()"
                    }
                ],
                "autoscale": true,
                "text_align": "center",
                "precision": 0
            },
            "layout": {
                "x": 4,
                "y": 1,
                "width": 2,
                "height": 2
            }
        },
        {
            "id": 5,
            "definition": {
                "type": "query_value",
                "title": "SECURITY BLOCKS",
                "title_size": "13",
                "title_align": "center",
                "requests": [
                    {
                        "q": "sum:sentinel.security.event{service:$service}.as_count()",
                        "aggregator": "sum",
                        "conditional_formats": [
                            {
                                "comparator": "<=",
                                "value": 5,
                                "palette": "white_on_green"
                            },
                            {
                                "comparator": ">",
                                "value": 5,
                                "palette": "white_on_yellow"
                            },
                            {
                                "comparator": ">",
                                "value": 20,
                                "palette": "white_on_red"
                            }
                        ]
                    }
                ],
                "autoscale": true,
                "text_align": "center",
                "precision": 0
            },
            "layout": {
                "x": 6,
                "y": 1,
                "width": 2,
                "height": 2
            }
        },
        {
            "id": 6,
            "definition": {
                "type": "query_value",
                "title": "SUCCESS RATE",
                "title_size": "13",
                "title_align": "center",
                "requests": [
                    {
                        "formulas": [
                            {
                                "formula": "(a / (a + b)) * 100"
                            }
                        ],
                        "queries": [
                            {
                                "data_source": "metrics",
                                "name": "a",
                                "query": "sum:sentinel.action.sent{service:$service}.as_count()"
                            },
                            {
                                "data_source": "metrics",
                                "name": "b",
                                "query": "sum:sentinel.action.failed{service:$service}.as_count()"
                            }
                        ],
                        "response_format": "scalar",
                        "conditional_formats": [
                            {
                                "comparator": ">=",
                                "value": 95,
                                "palette": "white_on_green"
                            },
                            {
                                "comparator": ">=",
                                "value": 85,
                                "palette": "white_on_yellow"
                            },
                            {
                                "comparator": "<",
                                "value": 85,
                                "palette": "white_on_red"
                            }
                        ]
                    }
                ],
                "autoscale": false,
                "custom_unit": "%",
                "text_align": "center",
                "precision": 1
            },
            "layout": {
                "x": 8,
                "y": 1,
                "width": 2,
                "height": 2
            }
        },
        {
            "id": 7,
            "definition": {
                "type": "query_value",
                "title": "E2E LATENCY P95",
                "title_size": "13",
                "title_align": "center",
                "requests": [
                    {
                        "q": "p95:sentinel.e2e.decision_latency_ms{service:$service}",
                        "conditional_formats": [
                            {
                                "comparator": "<=",
                                "value": 500,
                                "palette": "white_on_green"
                            },
                            {
                                "comparator": "<=",
                                "value": 1000,
                                "palette": "white_on_yellow"
                            },
                            {
                                "comparator": ">",
                                "value": 1000,
                                "palette": "white_on_red"
                            }
                        ]
                    }
                ],
                "autoscale": true,
                "custom_unit": "ms",
                "text_align": "center",
                "precision": 0
            },
            "layout": {
                "x": 10,
                "y": 1,
                "width": 2,
                "height": 2
            }
        },
        {
            "id": 8,
            "definition": {
                "type": "note",
                "content": "LATENCY & SLO PERFORMANCE",
                "background_color": "black",
                "font_size": "18",
                "text_align": "left",
                "vertical_align": "center",
                "show_tick": false,
                "has_padding": true
            },
            "layout": {
                "x": 0,
                "y": 3,
                "width": 12,
                "height": 1
            }
        },
        {
            "id": 9,
            "definition": {
                "type": "timeseries",
                "title": "End-to-End Decision Latency • p50 | p95 | p99",
                "title_size": "16",
                "title_align": "left",
                "requests": [
                    {
                        "q": "p50:sentinel.e2e.decision_latency_ms{service:$service}",
                        "display_type": "line",
                        "style": {
                            "palette": "cool",
                            "line_type": "solid",
                            "line_width": "thick"
                        },
                        "metadata": [
                            {
                                "expression": "p50:sentinel.e2e.decision_latency_ms{service:$service}",
                                "alias_name": "p50 Median"
                            }
                        ]
                    },
                    {
                        "q": "p95:sentinel.e2e.decision_latency_ms{service:$service}",
                        "display_type": "line",
                        "style": {
                            "palette": "warm",
                            "line_type": "solid",
                            "line_width": "thick"
                        },
                        "metadata": [
                            {
                                "expression": "p95:sentinel.e2e.decision_latency_ms{service:$service}",
                                "alias_name": "p95 Target"
                            }
                        ]
                    },
                    {
                        "q": "p99:sentinel.e2e.decision_latency_ms{service:$service}",
                        "display_type": "line",
                        "style": {
                            "palette": "orange",
                            "line_type": "solid",
                            "line_width": "thick"
                        },
                        "metadata": [
                            {
                                "expression": "p99:sentinel.e2e.decision_latency_ms{service:$service}",
                                "alias_name": "p99 Worst"
                            }
                        ]
                    }
                ],
                "markers": [
                    {
                        "value": "y = 500",
                        "display_type": "ok dashed",
                        "label": "SLO Target 500ms"
                    },
                    {
                        "value": "y = 1000",
                        "display_type": "error dashed",
                        "label": "Critical 1000ms"
                    }
                ],
                "yaxis": {
                    "include_zero": true,
                    "label": "Latency (ms)"
                },
                "show_legend": true,
                "legend_layout": "horizontal"
            },
            "layout": {
                "x": 0,
                "y": 4,
                "width": 8,
                "height": 4
            }
        },
